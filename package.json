{
    "name": "tx-monitor-job",
    "version": "1.0.0",
    "main": "./main.js",
    "description": "Monitor JOBS",
    "author": "Ternium MX <unindented@gmail.com>",
    "build": {
        "appId": "com.example.MonitorJobTX",
        "productName": "Monitor Job TX",
        "files": [
            "**/*",
            "dist/**/*"
        ],
        "directories": {
            "output": "release",
            "buildResources": "dist"
        },
        "asar": true,
        "win": {
            "target": "NSIS",
            "icon": "build/icon.ico"
        },
        "nsis": {
            "runAfterFinish": true
        }
    },
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build --prod",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "electron-build": "ng build --prod && electron .",
        "electron": "electron .",
        "pack": "build --dir",
        "dist": "build",
        "postinstall": "install-app-deps",
        "electron-package": "electron-packager . MonitoJobTX --platform=win32 --arch=x64 --overwrite"
    },
    "private": true,
    "dependencies": {
        "@amcharts/amcharts4": "^4.5.3",
        "@angular/animations": "^8.1.0",
        "@angular/cdk": "^8.0.2",
        "@angular/common": "^8.1.0",
        "@angular/compiler": "^8.1.0",
        "@angular/core": "^8.1.0",
        "@angular/fire": "^5.2.1",
        "@angular/forms": "^8.1.0",
        "@angular/platform-browser": "^8.1.0",
        "@angular/platform-browser-dynamic": "^8.1.0",
        "@angular/pwa": "^0.800.6",
        "@angular/router": "^8.1.0",
        "@angular/service-worker": "^8.1.0",
        "@iplab/ngx-file-upload": "^1.4.0",
        "@ng-bootstrap/ng-bootstrap": "^4.2.1",
        "@ng-select/ng-select": "^2.20.3",
        "@swimlane/ngx-charts": "^12.0.1",
        "angular-ng-autocomplete": "^1.1.13",
        "bootstrap": "^4.3.1",
        "chart.js": "^2.8.0",
        "core-js": "^3.1.4",
        "firebase": "^6.2.4",
        "jquery": "^3.5.0",
        "ng-pick-datetime": "^7.0.0",
        "ng-pick-datetime-moment": "^1.0.7",
        "ng2-charts": "^2.3.0",
        "ngx-pagination": "^4.0.0",
        "ngx-pipes": "^2.5.1",
        "ngx-ui-loader": "^8.0.0",
